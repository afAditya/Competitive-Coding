
#include<iostream>

 using namespace std;
   int main()

{
	int x1,v1,x2,v2;
	cin>>x1>>v1>>x2>>v2;
	
  if((x2>x1 && v1>v2) || (x1>x2 && v2>v1))
  {
	if((x2-x1)%(v1-v2)==0 || ((x1-x2)%(v2-v1)==0))
	{
		cout<<"Yes"<<endl;
	}
  

         else
		{
		  cout<<"No"<<endl;
		}
	
	

 }
	else
	{
		cout<<"No Outsidel"<<endl;
	}
}
